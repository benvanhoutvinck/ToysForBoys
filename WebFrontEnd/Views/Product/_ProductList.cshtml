@model IEnumerable<WebFrontEnd.Models.ProductListItem>

<div class="container">
    <div id="products" class="row list-group">
           @if(Model.Count() == 0)
           {
               <div>
                   No products were found by your search criteria
               </div>
           }

           @foreach (var item in Model)
           {
                @Html.Partial("_Detail", item)
           }
    </div>
</div>


